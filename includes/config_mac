SERVER=localhost
SERVER_IP=127.0.0.1

#DocumentRoot
WWW=/Users/$USER/Sites

#Apache directory
APACHEDIR=/etc/apache2

#Hosts file
HOSTS=/etc/hosts

#Vhost directory
VHOST=$APACHEDIR/other

#Apache restart command
APACHE_RESTART="sudo apachectl restart"

#MySQL user
MYSQL_USER=workspace

#dev git repository
REPOS[0]=git@github.com:mediainsights/IP.git
REPOS[1]=git@github.com:KelvinWong/IP.git


REPOS_SHORTNAME[0]=in
REPOS_SHORTNAME[1]=kw

#preset
PRESET_BRANCH=7.x-3.x
MANUAL_INSTALL=0
SKIP_DRUPAL=0
PROFILE=ip