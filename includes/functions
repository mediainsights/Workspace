#!/bin/bash

function check_db_exist {
	DBS=`mysql -uroot -p$rootpass -Bse 'show databases'| egrep -v 'information_schema|mysql'`
	for db in $DBS; do
		if [ "$db" = $1 ]; then
		    echo 1
			return
		fi
	done

	echo 0

}


#push workspace
function pushws {
	git co test
	git pull kw test
	
	git merge kelvin
	
	git reset kw/test
	
	git add .
	git add -u
	
	git commit
	
	git tag -a $1 -m \'version $1\'
} 