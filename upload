#!/bin/bash 

############################################################################# 
# Source files
#############################################################################

source ~/.bash_profile
source includes/config
source includes/functions

############################################################################# 
# Check usage
#############################################################################

#Usage check
: ${1?"Usage: $0 SITE [live | dev]"}


if [ $2 = live ]; then 
	drush rsync --include-conf @$1 @$1.$2 -y 
fi